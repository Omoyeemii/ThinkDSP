    {
      "cell_type": "code",
      "source": [
        "\n",
        "from scipy.signal import butter, lfilter\n",
        "\n",
        "def butter_filter(audio, cutoff, sr, btype, order=5):\n",
        "    nyquist = 0.5 * sr\n",
        "    norm_cutoff = cutoff / nyquist\n",
        "    b, a = butter(order, norm_cutoff, btype=btype)\n",
        "    return lfilter(b, a, audio)\n",
        "\n",
        "def bass_boost(audio, sr, gain=3):\n",
        "    bass = butter_filter(audio, cutoff=200, sr=sr, btype='low')\n",
        "    return audio + gain * bass\n",
        "\n",
        "def treble_boost(audio, sr, gain=3):\n",
        "    treble = butter_filter(audio, cutoff=3000, sr=sr, btype='high')\n",
        "    return audio + gain * treble"
      ],
      "metadata": {
        "id": "ouJHmXwbwBMv"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "bass_audio = bass_boost(audio, sr, gain=4)\n",
        "sf.write(\"bass_boost.wav\", bass_audio, sr)\n",
        "\n",
        "print(\"Bass boost done! File saved as bass_boost.wav\")"
