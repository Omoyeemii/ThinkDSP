        "import librosa\n",
        "import numpy as np\n",
        "import soundfile as sf\n",
        "\n",
        "audio, sr = librosa.load(\"c.wav\", sr=None)\n",
        "\n",
        "print(\"Audio loaded!\")\n",
        "print(\"Sample rate:\", sr)\n",
        "print(\"Length (seconds):\", len(audio)/sr)"
